{
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "less.lint.unknownAtRules": "ignore",
  "files.exclude": {
    "frontend/**": true,
    "frontend-react/**": true,
    "frontend-new/**": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "frontend/**": true,
    "frontend-react/**": true,
    "frontend-new/**": true,
    "**/node_modules": true
  },
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.suggest.autoImports": false,
  "files.watcherExclude": {
    "frontend/**": true,
    "frontend-react/**": true,
    "frontend-new/**": true,
    "**/node_modules": true
  }
}
